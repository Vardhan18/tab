{
  "manifest_version": 2,
  "name": "Tab Sorter",
  "version": "1.0.0",
  "description": "Rearrange your tabs by URL",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
     "default_title": "Open my URLs"
   },
"permissions": [
    "tabs"
  ],
  "options_page": "options.html"

  }